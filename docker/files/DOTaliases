alias beluga_demo_build='cd ~/ws \
  && colcon build --symlink-install \
  && source install/setup.bash'

alias beluga_demo_launch_beluga_ros_hallway_beam='cd ~/ws \
  && source install/setup.bash \
  && ros2 launch beluga_demo_bringup demo_beluga_ros_hallway_beam.launch.py'

alias beluga_demo_launch_beluga_ros_hallway_likelihood='cd ~/ws \
  && source install/setup.bash \
  && ros2 launch beluga_demo_bringup demo_beluga_ros_hallway_likelihood.launch.py'

