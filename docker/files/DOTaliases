  && source install/setup.bash \
  && ros2 launch beluga_demo_bringup demo_beluga_ros_office_beam.launch.py'

alias beluga_ros_office_likelihood_demo='cd ~/ws \
  && source install/setup.bash \
  && ros2 launch beluga_demo_bringup demo_beluga_ros_office_likelihood.launch.py'

